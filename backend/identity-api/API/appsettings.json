{
  "Sentry": {
    "Dsn": "",
    "IncludeRequestPayload": true,
    "SendDefaultPii": true,
    "MinimumBreadcrumbLevel": "Debug",
    "MinimumEventLevel": "Error",
    "AttachStackTrace": true,
    "Debug": true,
    "DiagnosticsLevel": "Error"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5435;Database=ticketmanagement;Username=postgres;Password=${SECRET}"
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "ValidIssuer": "TicketManagementAPI",
    "ValidAudience": "TicketManagementClient",
    "Expires": "60"
  }
}