{
  "Sentry": {
    "Dsn": "",
    "IncludeRequestPayload": true,
    "SendDefaultPii": true,
    "MinimumBreadcrumbLevel": "Debug",
    "MinimumEventLevel": "Error",
    "AttachStackTrace": true,
    "Debug": true,
    "DiagnosticsLevel": "Error"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "Issuer": "TicketManagementAPI",
    "Audience": "TicketManagementClient"
  },
  "Redis": {
    "ConnectionString": "localhost:6379"
  },
  "ReverseProxy": {
    "Routes": {
      "identity-route": {
        "ClusterId": "identity-cluster",
        "Match": {
          "Path": "/api/User/{**catch-all}"
        },
        "RetryPolicy": "DefaultRetryPolicy"
      },
      "auth-route": {
        "ClusterId": "identity-cluster",
        "Match": {
          "Path": "/api/Auth/{**catch-all}"
        },
        "RetryPolicy": "DefaultRetryPolicy"
      },
      "role-route": {
        "ClusterId": "identity-cluster",
        "Match": {
          "Path": "/api/Role/{**catch-all}"
        },
        "RetryPolicy": "DefaultRetryPolicy"
      },
      "identity-swagger-route": {
        "ClusterId": "identity-cluster",
        "Match": {
          "Path": "/swagger/identity/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/swagger/{**catch-all}"
          }
        ]
      }
    },
    "Clusters": {
      "identity-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "https://ticket-backend-identity:8443/"
          }
        },
        "HttpClient": {
          "DangerousAcceptAnyServerCertificate": true
        },
        "HttpRequest": {
          "ActivityTimeout": "00:00:10"
        }
      }
    }
  }
}